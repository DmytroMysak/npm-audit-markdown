{
  "name": "@netly/npm-audit-markdown",
  "version": "1.0.2",
  "description": "Generate a markdown report for NPM Audit",
  "main": "dist/src/index.js",
  "type": "module",
  "bin": {
    "@netly/npm-audit-markdown": "./dist/src/index.js"
  },
  "files": [
    "bin",
    "dist",
    "LICENSE",
    "package.json",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "start": "node dist/src/index.js",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "cz": "cz",
    "update": "ncu -u",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "chalk": "5.3.0",
    "commander": "11.0.0",
    "dot": "1.1.3"
  },
  "devDependencies": {
    "@netly/eslint-config-base": "^1.0.3",
    "@netly/prettier-config": "^1.1.0",
    "@netly/semantic-release-config": "^1.0.2",
    "@types/chai": "4.3.5",
    "@types/dot": "1.1.5",
    "@types/mocha": "10.0.1",
    "chai": "4.3.7",
    "mocha": "10.2.0",
    "mock-fs": "5.2.0",
    "npm-check-updates": "^16.10.15",
    "rimraf": "^5.0.1",
    "ts-node": "10.9.1",
    "typescript": "^5.1.6"
  },
  "prettier": "@netly/prettier-config",
  "engines": {
    "node": ">=16"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DmytroMysak/npm-audit-markdown"
  },
  "bugs": {
    "url": "https://github.com/DmytroMysak/npm-audit-markdown/issues"
  },
  "homepage": "https://github.com/DmytroMysak/npm-audit-markdown#readme",
  "keywords": [
    "npm audit",
    "npm audit markdown",
    "npm audit markdown report"
  ],
  "author": "Dmytro Mysak <dmytro.mysak@gmail.com>",
  "license": "MIT"
}
